  <select id="selectWorkInfoByWordId" resultMap="WorkInfoMapLeft">
    select
    wk.work_id,
    wk.process_number,
    wk.rating,
    pd.product_id,
    pd.product_name,
    pd.product_type,
    pd.image,
    pd.note,
    pd.status,
    pr.process_id,
    pr.technology_plan_id,
    pr.sequence,
    pr.quota,
    dv.device_id,
    dv.device_name,
    dv.device_type_id,
    dv.device_status_id,
    dv.device_status,
    dv.device_purchase_date,
    dv.device_purchase_price,
    dv.device_manufacture_date,
    dv.device_service_life,
    dv.device_keeper_id,
    dv.note note2
    from work wk
    left join product pd on wk.product_id = pd.product_id
    left join process pr on wk.process_id = pr.process_id
    left join device dv on wk.device_id = dv.device_id
    where wk.work_id = #{workId}
  </select>
